
package com.semanticsquare.collection;
import java.util.LinkedList;
public class LinkedListDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
LinkedList<String>Name =new LinkedList<>();

Name.add("Raj");
Name.add("Kiru");
Name.add("Ram");

System.out.println(Name);
Name.addFirst("Andi");      //AddFirst name
System.out.print(Name);     

System.out.println(Name);
Name.addLast("Suna");       //last name add
System.out.println(Name);   

System.out.println(Name);
Name.remove();                   //remove the First name FIFO
System.out.println(Name);    

System.out.println(Name);
Name.removeLast();
System.out.println(Name);       //remove the last name


for(String str:Name)
{
	System.out.println(str);
}
	}

}

o/p
[Raj, Kiru, Ram, Suna]
[Raj, Kiru, Ram, Suna]
[Raj, Kiru, Ram]
Raj
Kiru
Ram
